@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix purl:    <http://www.purl.org/> .
@prefix ogit.core:    <http://www.purl.org/ogit/> .

#
# Ontology
#
purl:ogit
    a owl:Ontology;
    rdfs:label "OGIT Base";
    dcterms:description """This ontology contains basic terms that are used to define Entities, Attributes and Verbs, but are not Verbs or Attributes themselves."""@en;
    dcterms:creator "arago GmbH";
    dcterms:contributor "Andreas Textor";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.core";
    owl:versionInfo "__OGIT_VERSION__";
.

#
# Entities
#
ogit.core:Attribute
    rdfs:subClassOf owl:DatatypeProperty;
    rdfs:subClassOf ogit.core:Entity;
    rdfs:label "Attribute";
    dcterms:description "Represents the class of all Attributes"@en;
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:hide "true";
    ogit.core:mandatory-attributes
        rdfs:label,
        dcterms:description,
        dcterms:valid,
        dcterms:creator,
        dcterms:created,
        dcterms:modified,
        ogit.core:admin-contact,
        ogit.core:tech-contact;
    ogit.core:optional-attributes
        ogit.core:deleter,
        ogit.core:hide,
        ogit.core:validation-type,
        ogit.core:validation-parameter;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

ogit.core:Entity
    a rdfs:Class;
    rdfs:subClassOf rdfs:Class;
    rdfs:subClassOf ogit.core:Entity;
    rdfs:label "Entity";
    dcterms:description """Represents the class of all Entities"""@en;
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:hide "true";
    ogit.core:mandatory-attributes
        rdfs:label,
        ogit.core:scope,
        dcterms:description,
        dcterms:valid,
        dcterms:creator,
        dcterms:modified,
        ogit.core:admin-contact,
        ogit.core:tech-contact,
        dcterms:description,
        dcterms:created,
        ogit.core:parent;
    ogit.core:optional-attributes
        ogit.core:deleter,
        ogit.core:hide;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

ogit.core:Verb
    rdfs:subClassOf owl:ObjectProperty;
    rdfs:subClassOf ogit.core:Entity;
    rdfs:label "Verb";
    dcterms:description "Represents the class of all Verbs"@en;
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:hide "true";
    ogit.core:mandatory-attributes
        dcterms:description,
        rdfs:label,
        dcterms:valid,
        dcterms:creator,
        dcterms:created,
        dcterms:modified,
        ogit.core:tech-contact,
        ogit.core:admin-contact;
    ogit.core:optional-attributes
        ogit.core:deleter,
        ogit.core:hide,
        ogit.core:cardinality;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

#
# Verbs
#
ogit.core:any-attributes
    a ogit.core:Verb;
    rdfs:label "any-attributes";
    dcterms:description "The list of any Attributes for an Entity"@en .

ogit.core:allowed
    a ogit.core:Verb;
    rdfs:label "allowed";
    dcterms:description "Constrains ranges and domains for Verbs"@en .

ogit.core:from
    a ogit.core:Verb;
    rdfs:label "from";
    dcterms:description "Constrains domains for Verbs"@en .

ogit.core:history
    a ogit.core:Verb;
    rdfs:label "history";
    dcterms:description "This represents the change history of an Entity, Attribute or Verb"@en .

ogit.core:indexed-attributes
    a ogit.core:Verb;
    rdfs:label "indexed-attributes";
    dcterms:description "The list of indexed Attributes for an Entity"@en .

ogit.core:mandatory-attributes
    a ogit.core:Verb;
    rdfs:label "mandatory-attributes";
    dcterms:description "The list of mandatory Attributes for an Entity"@en .

ogit.core:optional-attributes
    a ogit.core:Verb;
    rdfs:label "optional-attributes";
    dcterms:description "The list of optional Attributes for an Entity"@en .

ogit.core:to
    a ogit.core:Verb;
    rdfs:label "to";
    dcterms:description "Constrains ranges for Verbs"@en .

#
# Attributes
#
ogit.core:admin-contact
    a ogit.core:Attribute;
    rdfs:label "admin-contact" .

ogit.core:cardinality
    a ogit.core:Attribute;
    rdfs:label "cardinality" .

ogit.core:deleter
    a ogit.core:Attribute;
    rdfs:label "deleter" .

ogit.core:hide
    a ogit.core:Attribute;
    rdfs:label "hide" .

ogit.core:parent
    a ogit.core:Attribute;
    rdfs:label "parent" .

ogit.core:scope
    a ogit.core:Attribute;
    rdfs:label "scope" .

ogit.core:tech-contact
    a ogit.core:Attribute;
    rdfs:label "tech-contact" .

ogit.core:validation-parameter
    a ogit.core:Attribute;
    rdfs:label "validation-parameter" .

ogit.core:validation-type
    a ogit.core:Attribute;
    rdfs:label "validation-type" .

#
# Annotations
#
ogit.core:original
    a owl:AnnotationProperty;
    rdfs:label "original";
    dcterms:description """In a vocabulary imported into OGIT, an ogit.core:original reference can be added to elements to
refer to the original URI of the element. It is less strict than owl:sameAs, but unlike rdfs:seeAlso, it should
always refer to a URI.""" .
