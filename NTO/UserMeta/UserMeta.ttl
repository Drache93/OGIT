@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix dcterms:        <http://purl.org/dc/terms/> .
@prefix vann:           <http://purl.org/vocab/vann/> .
@prefix purl:           <http://www.purl.org/> .
@prefix ogit:           <http://www.purl.org/ogit/> .
@prefix ogit.core:           <http://www.purl.org/ogit/> .
@prefix ogit.person:           <http://www.purl.org/ogit/> .
@prefix ogit.foundation:           <http://www.purl.org/ogit/> .
@prefix ogit.UserMeta:           <http://www.purl.org/ogit/UserMeta/> .

#
# Ontology
#
ogit:UserMeta
    a owl:Ontology;
    rdfs:label "OGIT UserMeta";
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/UserMeta/";
    vann:preferredNamespacePrefix "ogit.UserMeta";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports purl:ogit
.

#
# Entities
#
ogit.UserMeta:Filter
    a rdfs:Class;
    rdfs:subClassOf ogit.core:Entity;
    rdfs:label "Filter";
    dcterms:description "A filter set by user, indicating preferences";
    dcterms:valid "start=2016-07-19;";
    dcterms:creator "stravlos@arago.de";
    dcterms:created "2016-10-14";
    dcterms:modified "2016-10-14";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "NTO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:mandatory-attributes
        ogit.foundation:name,
        ogit.foundation:content;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2016-07-19";
            dcterms:description "Initial Creation";
            dcterms:creator "stravlos@arago.de"
        ];
.

ogit.UserMeta:Game
    a rdfs:Class;
    rdfs:subClassOf ogit.core:Entity;
    rdfs:label "Game";
    dcterms:description "Something which is played to obtain meta data around user behaviour and perception";
    dcterms:valid "start=2016-07-19;";
    dcterms:creator "bneal@arago.de";
    dcterms:created "2016-07-19";
    dcterms:modified "2016-07-19";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "NTO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2016-07-19";
            dcterms:description "Initial Creation";
            dcterms:creator "bneal@arago.de"
        ];
.

ogit.UserMeta:Question
    a rdfs:Class;
    rdfs:subClassOf ogit.core:Entity;
    rdfs:label "Question";
    dcterms:description "A question which is part of a game, played to obtain meta data around user behaviour and perception";
    dcterms:valid "start=2016-07-19;";
    dcterms:creator "bneal@arago.de";
    dcterms:created "2016-07-19";
    dcterms:modified "2016-07-19";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "NTO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2016-07-19";
            dcterms:description "Initial Creation";
            dcterms:creator "bneal@arago.de"
        ];
.


#
# Attributes
#

#
# Verbs
#
ogit.UserMeta:asks
    a ogit.core:Verb;
    rdfs:label "asks";
    dcterms:description "This verb represents that a games asks a question";
    dcterms:valid "start=2016-07-19;";
    dcterms:creator "bneal@arago.de";
    dcterms:created "2016-07-19";
    dcterms:modified "2016-07-19";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2016-07-19";
            dcterms:description "Initial Creation";
            dcterms:creator "bneal@arago.de"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.UserMeta:Game;
            ogit.core:to ogit.UserMeta:Question
        ];
.

