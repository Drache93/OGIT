            dcterms:date "2015-07-09";
            dcterms:description "change attribute ogit/startAt to ogit/startedAt and ogit/finishAt to ogit/finishedAt";
            dcterms:creator "Peter Larem"
        ],
        [
            dcterms:identifier "4";
            dcterms:date "2015-12-10";
