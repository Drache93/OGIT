@prefix dcterms:             <http://purl.org/dc/terms/> .
@prefix ogit:                <http://www.purl.org/ogit/> .
@prefix ogit.core:           <http://www.purl.org/ogit/> .
@prefix ogit.dataProcessing: <http://www.purl.org/ogit/DataProcessing/> .
@prefix ogit.foundation:     <http://www.purl.org/ogit/> .
@prefix owl:                 <http://www.w3.org/2002/07/owl#> .
@prefix purl:                <http://www.purl.org/> .
@prefix rdfs:                <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:                  <http://www.w3.org/ns/shacl#> .
@prefix vann:                <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:dataProcessing
    a owl:Ontology ;
    rdfs:label "OGIT Data Processing" ;
    owl:imports purl:ogit ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.dataProcessing" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/DataProcessing/" .

#
# Entities
#
ogit.dataProcessing:Program
    a ogit.core:Entity ;
    rdfs:label "DataProcessingProgram" ;
    dcterms:created "2017-02-28" ;
    dcterms:creator "Oday Jubran" ;
    dcterms:description """The program is used for creating data streams,
        and applying transformations to them.""" ;
    dcterms:modified "2017-02-28" ;
    dcterms:valid "start=2017-02-28;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Oday Jubran" ;
            dcterms:date "2017-02-28" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:indexed-attributes
        ogit.foundation:description,
        ogit.foundation:name ;
    ogit.core:parent "http://www.purl.org/ogit/Node" ;
    ogit.core:scope "NTO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.dataProcessing:queryType
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.dataProcessing:query
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.dataProcessing:parameters
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.dataProcessing:outputType
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:content
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:description
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.dataProcessing:internalJobId
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.dataProcessing:state
        ] .

