#
# Ontology
#
ogit:graphit
    a owl:Ontology;
    rdfs:label "OGIT GraphIT";
    owl:imports purl:ogit;
    owl:versionInfo "__OGIT_VERSION__";
    dcterms:contributor "Fabian Meyer";
    dcterms:creator "arago GmbH";
    dcterms:description "This ontology contains terms, internally used by GraphIT."@en;
    vann:preferredNamespacePrefix "ogit.graphit";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit.graphit:Node
    a ogit.core:Entity;
    rdfs:label "Node";
    dcterms:created "2013-09-01";
    dcterms:creator "Peter Larem";
    dcterms:description """The root node, the 'Big Bang'. The nodes Global, Factual, Actionable and Situational build with this Node the
        top level of the SGO.""";
    dcterms:modified "2013-09-01";
    dcterms:valid "start=2013-11-01;";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:hide "true";
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2013-09-01" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ];
    ogit.core:scope "SGO";
    ogit.core:tech-contact "arago GmbH";
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_id
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_type
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_owner
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_modified-on
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_modified-by
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_graphtype
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_is-deleted
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_deleted-on
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_creator
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_created-on
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_version
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_content
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_tags
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.graphit:_edge_id
        ] .

