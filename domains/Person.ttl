ogit.person:Vcard
    a ogit.core:Entity ;
    rdfs:label "Vcard" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description """vCard is a specification developed by the IETF for the description of people and organisations. Recently, vCard has been
        significantly updated to Version 4 as documented in [RFC6350]. Typically, vCard objects are encoded in its own defined
        text-based syntax or XML renderings.""" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:parent "http://www.purl.org/ogit/Node" ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:phone
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.person:mobilePhone
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:streetAddress
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.person:addressLocality
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:postalCode
        ] .

