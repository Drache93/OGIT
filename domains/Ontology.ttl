#
# Ontology
#
ogit:ontology
    a owl:Ontology;
    rdfs:label "OGIT Ontology";
    owl:imports purl:ogit;
    owl:versionInfo "__OGIT_VERSION__";
    dcterms:contributor "Fabian Meyer";
    dcterms:creator "arago GmbH";
    dcterms:description "This ontology contains terms that are used to describe OGIT entities, attributes and verbs."@en;
    vann:preferredNamespacePrefix "ogit.ontology";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

