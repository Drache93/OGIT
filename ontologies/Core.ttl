@prefix dcterms:   <http://purl.org/dc/terms/> .
@prefix ogit.core: <http://www.purl.org/ogit/> .
@prefix owl:       <http://www.w3.org/2002/07/owl#> .
@prefix purl:      <http://www.purl.org/> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:        <http://www.w3.org/ns/shacl#> .
@prefix vann:      <http://purl.org/vocab/vann/> .

#
# Ontology
#
purl:ogit
    a owl:Ontology ;
    rdfs:label "OGIT Base" ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Andreas Textor" ;
    dcterms:creator "arago GmbH" ;
    dcterms:description "This ontology contains basic terms that are used to define Entities, Attributes and Verbs, but are not Verbs or Attributes themselves."@en ;
    vann:preferredNamespacePrefix "ogit.core" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit.core:Attribute
    rdfs:label "Attribute" ;
    rdfs:subClassOf
        ogit.core:Entity,
        owl:DatatypeProperty ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Represents the class of all Attributes"@en ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:hide "true" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdfs:label
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path dcterms:description
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path dcterms:valid
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path dcterms:creator
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path dcterms:created
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path dcterms:modified
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.core:admin-contact
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.core:tech-contact
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.core:deleter
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.core:hide
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.core:validation-type
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.core:validation-parameter
        ] .

