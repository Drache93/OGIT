#
# Attributes
#
ogit.core:admin-contact
    a ogit.core:Attribute ;
    rdfs:label "admin-contact" .

ogit.core:cardinality
    a ogit.core:Attribute ;
    rdfs:label "cardinality" .

ogit.core:deleter
    a ogit.core:Attribute ;
    rdfs:label "deleter" .

ogit.core:hide
    a ogit.core:Attribute ;
    rdfs:label "hide" .

ogit.core:parent
    a ogit.core:Attribute ;
    rdfs:label "parent" .

ogit.core:scope
    a ogit.core:Attribute ;
    rdfs:label "scope" .

ogit.core:tech-contact
    a ogit.core:Attribute ;
    rdfs:label "tech-contact" .

ogit.core:validation-parameter
    a ogit.core:Attribute ;
    rdfs:label "validation-parameter" .

ogit.core:validation-type
    a ogit.core:Attribute ;
    rdfs:label "validation-type" .

#
# Verbs
#
ogit.core:any-attributes
    a ogit.core:Verb ;
    rdfs:label "any-attributes" ;
    dcterms:description "The list of any Attributes for an Entity"@en .

ogit.core:history
    a ogit.core:Verb ;
    rdfs:label "history" ;
    dcterms:description "This represents the change history of an Entity, Attribute or Verb"@en .

#
# Other
#
ogit.core:original
    a owl:AnnotationProperty ;
    rdfs:label "original" ;
    dcterms:description """In a vocabulary imported into OGIT, an ogit.core:original reference can be added to elements to
        refer to the original URI of the element. It is less strict than owl:sameAs, but unlike rdfs:seeAlso, it should
        always refer to a URI.""" .