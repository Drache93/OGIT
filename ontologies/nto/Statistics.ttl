@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix ogit.statistics: <http://www.purl.org/ogit/Statistics/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:statistics
    a owl:Ontology ;
    rdfs:label "OGIT Statistics" ;
    owl:imports ogit:foundation ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.statistics" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/Statistics/" .

#
# Entities
#
ogit.statistics:AutomationFamily
    a ogit.core:Entity ;
    rdfs:label "AutomationFamily" ;
    dcterms:created "2016-03-07" ;
    dcterms:creator "Liudmyla Nechepurenko" ;
    dcterms:description "AutomationFamily defines the group of cases for automation of IT environment and its functionality." ;
    dcterms:modified "2016-03-07" ;
    dcterms:valid "start=2016-03-07;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:scope "NTO" ;
    ogit.core:tech-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier 1 ;
            dcterms:date "2016-03-07" ;
            dcterms:creator "Liudmyla Nechepurenko" ;
            dcterms:description "initial"
        ] ;
    sh:property
        [
            sh:path ogit.foundation:category ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:description ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:name ;
            sh:minCount 1 ;
            sh:maxCount 1
        ] .
