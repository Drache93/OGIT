@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix ogit.price:      <http://www.purl.org/ogit/Price/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:price
    a owl:Ontology ;
    rdfs:label "OGIT Price" ;
    owl:imports ogit:foundation ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.price" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/Price/" .

#
# Entities
#
ogit.price:Invoice
    a ogit.core:Entity ;
    rdfs:label "Invoice" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Models any type of invoice" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "NTO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:currency
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.price:priceAmount
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:description
        ] .

ogit.price:PriceSpecification
    a ogit.core:Entity ;
    rdfs:label "PriceSpecification" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Models a price specification for any type of IT service, component or task" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "NTO" ;
    ogit.core:tech-contact "arago GmbH" .

