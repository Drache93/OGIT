#
# Ontology
#
ogit:oslc-reqman
    a owl:Ontology ;
    rdfs:label "OGIT OSLC Requirements Management" ;
    owl:imports purl:ogit ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.oslc-reqman" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/OSLC-reqman/" .

