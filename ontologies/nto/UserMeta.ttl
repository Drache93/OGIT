        ] .

ogit.userMeta:wins
    a ogit.core:Verb ;
    rdfs:label "wins" ;
    dcterms:created "2016-07-19" ;
    dcterms:creator "bneal@arago.de" ;
    dcterms:description "This verb represents that a user has won a game" ;
