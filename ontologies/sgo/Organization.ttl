#
# Ontology
#
ogit:organization
    a owl:Ontology ;
    rdfs:label "OGIT Organization" ;
    owl:imports
        ogit:_data,
        ogit:alerting,
        ogit:assets,
        ogit:catalog,
        ogit:configuration,
        ogit:contract,
        ogit:device,
        ogit:foundation,
        ogit:graphit,
        ogit:license,
        ogit:location,
        ogit:person ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._organization" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit._organization:Organization
    a ogit.core:Entity ;
    rdfs:label "Organization" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description """An identity that represents an organization, company, or some other grouping of Person objects.
Entities of this type should be created with a predefined ID. Those IDs should be named like a domain name, i.e.
a concatention of some strings separated with a dot. if organization data is created from a LDAP then a mapping
like "cn=John Doe,dc=example,dc=com" => "john_doe.example.com" should be employed.
The ogit/function attribute should be used to tell what kind of grouping an entity of type Organization is: company, department, group, ...""" ;
    dcterms:modified "2015-07-27" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:any-attributes "true" ;
    ogit.core:history
        [
            dcterms:creator "Aymen Ayoub" ;
            dcterms:date "2015-10-15" ;
            dcterms:description "added optional attribute ogit/webPage" ;
            dcterms:identifier "3"
        ],
        [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ],
        [
            dcterms:creator "Philipp Pelchmann" ;
            dcterms:date "2015-07-27" ;
            dcterms:description "added optional attributes ogit/streetAddress, ogit/city, ogit/country, ogit/postalCode, ogit/businessCategory" ;
            dcterms:identifier "2"
        ] ;
    ogit.graphit:indexed-attributes
        ogit.foundation:description,
        ogit.foundation:name ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:path ogit._location:locatedIn ;
            sh:class ogit._location:Location
        ],
        [
            sh:path ogit._organization:businessCategory ;
            sh:maxCount 1
        ],
        [
            sh:path ogit._organization:city ;
            sh:maxCount 1
        ],
        [
            sh:path ogit._organization:competes ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit._organization:country ;
            sh:maxCount 1
        ],
        [
            sh:path ogit._organization:deliversTo ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit._organization:sellsTo ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:approves ;
            sh:class ogit._configuration:ConfigurationItem
        ],
        [
            sh:path ogit.foundation:associates ;
            sh:class ogit._organization:Role
        ],
        [
            sh:path ogit.foundation:belongs ;
            sh:class ogit._alerting:Alert
        ],
        [
            sh:path ogit.foundation:communicatesWith ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:complies ;
            sh:class ogit._organization:Policy
        ],
        [
            sh:path ogit.foundation:concludes ;
            sh:class ogit._contract:Contract
        ],
        [
            sh:path ogit.foundation:contains ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:creates ;
            sh:class ogit._configuration:ConfigurationItem
        ],
        [
            sh:path ogit.foundation:creates ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:defines ;
            sh:class ogit._organization:Role
        ],
        [
            sh:path ogit.foundation:description ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:email ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:employs ;
            sh:class ogit._person:Person
        ],
        [
            sh:path ogit.foundation:function ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:generates ;
            sh:class ogit._catalog:Catalog
        ],
        [
            sh:path ogit.foundation:generates ;
            sh:class ogit._data:Timeseries
        ],
        [
            sh:path ogit.foundation:governs ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:leads ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:manages ;
            sh:class ogit._configuration:ConfigurationItem
        ],
        [
            sh:path ogit.foundation:manages ;
            sh:class ogit._device:Device
        ],
        [
            sh:path ogit.foundation:manages ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:name ;
            sh:minCount 1 ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:owns ;
            sh:class ogit._asset:Asset
        ],
        [
            sh:path ogit.foundation:phone ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:postalCode ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:provides ;
            sh:class ogit._configuration:ConfigurationItem
        ],
        [
            sh:path ogit.foundation:relates ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:represents ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:represents ;
            sh:class ogit._person:Person
        ],
        [
            sh:path ogit.foundation:sourceId ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:status ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:streetAddress ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:supports ;
            sh:class ogit._configuration:ConfigurationItem
        ],
        [
            sh:path ogit.foundation:supports ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:uses ;
            sh:class ogit._license:License
        ],
        [
            sh:path ogit.foundation:webPage ;
            sh:maxCount 1
        ],
