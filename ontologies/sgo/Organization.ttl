#
# Ontology
#
ogit:organization
    a owl:Ontology ;
    rdfs:label "OGIT Organization" ;
    owl:imports
        ogit:_data,
        ogit:alerting,
        ogit:assets,
        ogit:catalog,
        ogit:configuration,
        ogit:contract,
        ogit:device,
        ogit:foundation,
        ogit:graphit,
        ogit:license,
        ogit:person ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._organization" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

