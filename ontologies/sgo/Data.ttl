@prefix dcterms:            <http://purl.org/dc/terms/> .
@prefix ogit:               <http://www.purl.org/ogit/> .
@prefix ogit._data:         <http://www.purl.org/ogit/> .
@prefix ogit._organization: <http://www.purl.org/ogit/> .
@prefix ogit.core:          <http://www.purl.org/ogit/> .
@prefix ogit.foundation:    <http://www.purl.org/ogit/> .
@prefix ogit.graphit:       <http://www.purl.org/ogit/> .
@prefix owl:                <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:               <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:                 <http://www.w3.org/ns/shacl#> .
@prefix vann:               <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:_data
    a owl:Ontology ;
    rdfs:label "OGIT Data" ;
    owl:imports
        ogit:foundation,
        ogit:graphit,
        ogit:organization ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._data" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit._data:Certificate
    a ogit.core:Entity ;
    rdfs:label "Certificate" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2017-09-05" ;
    dcterms:creator "Oday Jubran" ;
    dcterms:description "This enables storing information about existing certificates as vertices." ;
    dcterms:modified "2017-09-05" ;
    dcterms:valid "start=2017-09-05;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier "1" ;
            dcterms:date "2017-09-05" ;
            dcterms:creator "Oday Jubran" ;
            dcterms:description "initial"
        ] ;
    sh:property
        [
            sh:path ogit.foundation:belongs ;
            sh:class ogit._organization:Organization
        ],
        [
            sh:path ogit.foundation:id ;
            sh:minCount 1 ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:subject ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:validFrom ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:validTo ;
            sh:maxCount 1
        ] .

ogit._data:CustomApplicationData
    a ogit.core:Entity ;
    rdfs:label "CustomApplicationData" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description """Entity to store custom application data, a storage place not connected to any other entity and therefore isolated from
the global ontology. The storage of data through this entity is discouraged due to low performance.""" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier "1" ;
            dcterms:date "2015-05-21" ;
            dcterms:creator "Peter Larem" ;
            dcterms:description "initial"
        ] ;
    sh:property [
            sh:path ogit.foundation:uses ;
            sh:class ogit._data:CustomApplicationData
        ] .

ogit._data:Timeseries
    a ogit.core:Entity ;
    rdfs:label "Timeseries" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "A time series is a sequence of data points, measured typically at successive points in time spaced at uniform time intervals." ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    ogit.graphit:indexed-attributes
        ogit.foundation:description,
        ogit.foundation:instance,
        ogit.foundation:name ;
    ogit.core:history [
            dcterms:identifier "1" ;
            dcterms:date "2015-05-21" ;
            dcterms:creator "Peter Larem" ;
            dcterms:description "initial"
        ] ;
    sh:property
        [
            sh:path ogit.foundation:description ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:instance ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:name ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:unit ;
            sh:maxCount 1
        ] .
