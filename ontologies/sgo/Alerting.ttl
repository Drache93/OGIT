@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit._alerting:  <http://www.purl.org/ogit/> .
@prefix ogit._contract:  <http://www.purl.org/ogit/> .
@prefix ogit._event:     <http://www.purl.org/ogit/> .
@prefix ogit._license:   <http://www.purl.org/ogit/> .
@prefix ogit._person:    <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix ogit.graphit:    <http://www.purl.org/ogit/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:alerting
    a owl:Ontology ;
    rdfs:label "OGIT Alerting" ;
    owl:imports
        ogit:contract,
        ogit:event,
        ogit:foundation,
        ogit:graphit,
        ogit:license,
        ogit:person ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._alerting" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit._alerting:Alert
    a ogit.core:Entity ;
    rdfs:label "Alert" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2016-10-26" ;
    dcterms:creator "stravlos@arago.de" ;
    dcterms:description "Alerts are used to notify users if certain events happen." ;
    dcterms:modified "2016-10-26" ;
    dcterms:valid "start=2016-10-26;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    ogit.core:history
        [
            dcterms:identifier 1 ;
            dcterms:date "2016-10-26" ;
            dcterms:creator "stravlos@arago.de" ;
            dcterms:description "initial"
        ],
        [
            dcterms:identifier 2 ;
            dcterms:date "2016-11-02" ;
            dcterms:creator "stravlos@arago.de" ;
            dcterms:description "make attributes optional"
        ] ;
    sh:property
        [
            sh:path ogit._alerting:alerts ;
            sh:class ogit._person:Person
        ],
        [
            sh:path ogit.foundation:category ;
            sh:minCount 1 ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:contains ;
            sh:class ogit._contract:Contract
        ],
        [
            sh:path ogit.foundation:contains ;
            sh:class ogit._license:License
        ],
        [
            sh:path ogit.foundation:creator ;
            sh:minCount 1 ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:function ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:triggers ;
            sh:class ogit._event:Event
        ],
        [
            sh:path ogit.foundation:type ;
            sh:minCount 1 ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:value ;
            sh:maxCount 1
        ] .

#
# Verbs
#
ogit._alerting:alerts
    a ogit.core:Verb ;
    rdfs:label "alerts" ;
    dcterms:created "2016-10-26" ;
    dcterms:creator "stravlos@arago.co" ;
    dcterms:description "Verb showing if one entity alerts another." ;
    dcterms:modified "2016-10-26" ;
    dcterms:valid "start=2016-10-26;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:tech-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier 1 ;
            dcterms:date "2016-10-26" ;
            dcterms:creator "stravlos@arago.co" ;
            dcterms:description "initial"
        ] .
