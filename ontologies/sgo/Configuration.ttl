#
# Ontology
#
ogit:configuration
    a owl:Ontology ;
    rdfs:label "OGIT Configuration" ;
    owl:imports
        ogit:assets,
        ogit:catalog,
        ogit:contract,
        ogit:device,
        ogit:foundation,
        ogit:graphit,
        ogit:license,
        ogit:media,
        ogit:organization,
        ogit:person ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._configuration" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

