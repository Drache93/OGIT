@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit._process:   <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:process
    a owl:Ontology ;
    rdfs:label "OGIT Process" ;
    owl:imports
        ogit:foundation,
        ogit:graphit ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._process" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit._process:Notification
    a ogit.core:Entity ;
    rdfs:label "Notification" ;
    dcterms:created "2015-04-27" ;
    dcterms:creator "cwalker@arago.de" ;
    dcterms:description "A notification event" ;
    dcterms:modified "2017-02-17" ;
    dcterms:valid "start=2015-04-27;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history
        [
            dcterms:creator "cwalker@arago.de" ;
            dcterms:date "2015-04-27" ;
            dcterms:description "Initial Creation" ;
            dcterms:identifier "1"
        ],
        [
            dcterms:creator "stravlos@arago.de" ;
            dcterms:date "2017-02-17" ;
            dcterms:description "Make description more general" ;
            dcterms:identifier "2"
        ] ;
    ogit.core:parent ogit:Node ;
    ogit.core:scope "NTO" ;
    ogit.core:tech-contact "arago GmbH" .

ogit._process:Question
    a ogit.core:Entity ;
    rdfs:label "Question" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description """This structure allows an automation engine to create data requests for existing Issues which should be filled by human user.
        Additionaly could be used to create new Issue from some form.""" ;
    dcterms:modified "2016-07-12" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history
        [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ],
        [
            dcterms:creator "Arthur Shoba" ;
            dcterms:date "2016-07-12" ;
            dcterms:description "Added new attribute `capacity`" ;
            dcterms:identifier "2"
        ] ;
    ogit.core:parent ogit:Node ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:status
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit._process:question
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit._process:response
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:capacity
        ] .

ogit._process:Task
    a ogit.core:Entity ;
    rdfs:label "Task" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "A process is a unit of execution." ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history
        [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ],
        [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-09-10" ;
            dcterms:description "add attribute ogit/positionNumber" ;
            dcterms:identifier "2"
        ] ;
    ogit:indexed-attributes ogit.foundation:name ;
    ogit.core:parent ogit:Node ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:id
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:description
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:status
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit._process:positionNumber
        ] .

ogit._process:TaskList
    a ogit.core:Entity ;
    rdfs:label "TaskList" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "A process list consists of one or many processs." ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit:indexed-attributes ogit.foundation:name ;
    ogit.core:parent ogit:Node ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:class ogit._process:Task ;
            sh:description "" ;
            sh:minCount 0 ;
            sh:path ogit.foundation:contains
        ] .

