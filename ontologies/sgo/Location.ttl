@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit._data:      <http://www.purl.org/ogit/> .
@prefix ogit._location:  <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix ogit.graphit:    <http://www.purl.org/ogit/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:location
    a owl:Ontology ;
    rdfs:label "OGIT Location" ;
    owl:imports
        ogit:_data,
        ogit:foundation,
        ogit:graphit ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._location" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit._location:Location
    a ogit.core:Entity ;
    rdfs:label "Location" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description """A place where some some service is offered or requested. In many cases this will be related to a an address of an 
Organization. Similar to http://purl.org/goodrelations/v1#Location""" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier "1" ;
            dcterms:date "2015-05-21" ;
            dcterms:creator "Peter Larem" ;
            dcterms:description "initial"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:path ogit._location:locatedIn ;
            sh:class ogit._location:Region
        ],
        [
            sh:path ogit.foundation:description ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:generates ;
            sh:class ogit._data:Timeseries
        ],
        [
            sh:path ogit.foundation:name ;
            sh:minCount 1 ;
            sh:maxCount 1
        ] .

ogit._location:Region
    a ogit.core:Entity ;
    rdfs:label "Region" ;
    rdfs:subClassOf ogit.graphit:Node ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "A region is always something that has a certain geographic extension (a country, a county,  ) whereas ogit/Location usually refers to specific place usually having an address" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier "1" ;
            dcterms:date "2015-05-21" ;
            dcterms:creator "Peter Larem" ;
            dcterms:description "initial"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:path ogit.foundation:description ;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:name ;
            sh:maxCount 1
        ] .

#
# Verbs
#
ogit._location:availableIn
    a ogit.core:Verb ;
    rdfs:label "availableIn" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Indicates that something can be found from somewhere." ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:identifier "1" ;
            dcterms:date "2015-05-21" ;
            dcterms:creator "Peter Larem" ;
            dcterms:description "initial"
        ] ;
    ogit.core:tech-contact "arago GmbH" .

