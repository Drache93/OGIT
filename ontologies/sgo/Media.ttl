@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit._media:     <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:media
    a owl:Ontology ;
    rdfs:label "OGIT Media" ;
    owl:imports ogit:foundation ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit._media" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit._media:Comment
    a ogit.core:Entity ;
    rdfs:label "Comment" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Defines comments for a ticket, issue or another type of entity. Also known as WorkLog, ChangeLog or HistoryEntry." ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history
        [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ],
        [
            dcterms:creator "Aymen Ayoub" ;
            dcterms:date "2015-08-20" ;
            dcterms:description "updated Attribute ogit/id and ogit/content added." ;
            dcterms:identifier "2"
        ],
        [
            dcterms:creator "Aymen Ayoub" ;
            dcterms:date "2015-09-01" ;
            dcterms:description "updated Attribute ogit/id to be optional." ;
            dcterms:identifier "3"
        ],
        [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-10-16" ;
            dcterms:description "added ogit/creater as optional." ;
            dcterms:identifier "4"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:class ogit._media:Comment ;
            sh:description "" ;
            sh:minCount 0 ;
            sh:path ogit.foundation:connects
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:id
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:content
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:creator
        ],
        [
            sh:class ogit._media:Hyperlink ;
            sh:description "" ;
            sh:minCount 0 ;
            sh:path ogit.foundation:connects
        ] .

ogit._media:Hyperlink
    a ogit.core:Entity ;
    rdfs:label "Hyperlink" ;
    dcterms:created "2015-06-09" ;
    dcterms:creator "cwalker@arago.de" ;
    dcterms:description "A representation of an (optionally) titled Hyperlink to some media. No protocol restrictions imposed." ;
    dcterms:modified "2015-06-09" ;
    dcterms:valid "start=2015-06-09;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:any-attributes "true" ;
    ogit.core:history [
            dcterms:creator "cwalker@arago.de" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:url
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:title
        ] .

ogit._media:Note
    a ogit.core:Entity ;
    rdfs:label "Note" ;
    dcterms:created "2015-04-29" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Any type of note." ;
    dcterms:modified "2013-04-29" ;
    dcterms:valid "start=2015-04-29;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-04-29" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:content
        ] .