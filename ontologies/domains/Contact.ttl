@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit.contact:    <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:contacts
    a owl:Ontology ;
    rdfs:label "OGIT Contact" ;
    owl:imports ogit:foundation ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.contact" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit.contact:Email
    a ogit.core:Entity ;
    rdfs:label "Email" ;
    dcterms:created "2016-01-22" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "Any type of email adress." ;
    dcterms:modified "2016-01-22" ;
    dcterms:valid "start=2016-01-22;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2016-01-22" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:status
        ] .

ogit.contact:Vcard
    a ogit.core:Entity ;
    rdfs:label "Vcard" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description """vCard is a specification developed by the IETF for the description of people and organisations. Recently, vCard has been
        significantly updated to Version 4 as documented in [RFC6350]. Typically, vCard objects are encoded in its own defined
        text-based syntax or XML renderings.""" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:phone
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:mobilePhone
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:streetAddress
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.contact:addressLocality
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:postalCode
        ] .

#
# Attributes
#
ogit.contact:addressLocality
    a ogit.core:Attribute ;
    rdfs:label "addressLocality" ;
    dcterms:created "2015-05-21" ;
    dcterms:creator "Peter Larem" ;
    dcterms:description "The locality belonging to an address. For example, Frankfurt am Main" ;
    dcterms:modified "2015-05-21" ;
    dcterms:valid "start=2015-05-21;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:tech-contact "arago GmbH" .