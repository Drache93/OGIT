#
# Ontology
#
ogit:rddl
    a owl:Ontology ;
    rdfs:label "OGIT Resource Directory Description Language" ;
    owl:imports purl:ogit ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.rddl" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/RDDL/" .

#
# Entities
#
ogit.rddl:Namespace
    a ogit.core:Entity ;
    rdfs:label "Namespace" ;
    dcterms:created "2016-09-22" ;
    dcterms:creator "OGIT Importer" ;
    dcterms:description "An XML namespace." ;
    dcterms:modified "2016-09-22" ;
    dcterms:valid "start=2016-09-22;" ;
    ogit.core:admin-contact "arago GmbH" ;
