        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyName
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyValidFrom
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyCreator
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyCreated
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyModified
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyAdminContact
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit._ontology:ontologyTechContact
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
