@prefix dcterms:         <http://purl.org/dc/terms/> .
@prefix ogit:            <http://www.purl.org/ogit/> .
@prefix ogit.core:       <http://www.purl.org/ogit/> .
@prefix ogit.course:     <http://www.purl.org/ogit/> .
@prefix ogit.foundation: <http://www.purl.org/ogit/> .
@prefix owl:             <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:            <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:              <http://www.w3.org/ns/shacl#> .
@prefix vann:            <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:course
    a owl:Ontology ;
    rdfs:label "OGIT Course" ;
    owl:imports ogit:foundation ;
    owl:versionInfo "__OGIT_VERSION__" ;
    dcterms:contributor "Fabian Meyer" ;
    dcterms:creator "arago GmbH" ;
    vann:preferredNamespacePrefix "ogit.course" ;
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Entities
#
ogit.course:Course
    a ogit.core:Entity ;
    rdfs:label "Course" ;
    dcterms:created "2017-04-19" ;
    dcterms:creator "stravlos@arago.de" ;
    dcterms:description "Any kind of course" ;
    dcterms:modified "2017-04-19" ;
    dcterms:valid "start=2017-04-19;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "stravlos@arago.de" ;
            dcterms:date "2017-04-19" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:id
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:description
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:status
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:creator
        ] .

ogit.course:Series
    a ogit.core:Entity ;
    rdfs:label "Series" ;
    dcterms:created "2017-04-19" ;
    dcterms:creator "stravlos@arago.de" ;
    dcterms:description "Any kind of series" ;
    dcterms:modified "2017-04-19" ;
    dcterms:valid "start=2017-04-19;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "stravlos@arago.de" ;
            dcterms:date "2017-04-19" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:name
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path ogit.foundation:id
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:description
        ],
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:creator
        ],
        [
            sh:class ogit.course:Course ;
            sh:description "" ;
            sh:minCount 0 ;
            sh:path ogit.foundation:contains
        ] .

ogit.course:Status
    a ogit.core:Entity ;
    rdfs:label "Status" ;
    dcterms:created "2017-04-19" ;
    dcterms:creator "stravlos@arago.de" ;
    dcterms:description "Any kind of status" ;
    dcterms:modified "2017-04-19" ;
    dcterms:valid "start=2017-04-19;" ;
    ogit.core:admin-contact "arago GmbH" ;
    ogit.core:history [
            dcterms:creator "stravlos@arago.de" ;
            dcterms:date "2017-04-19" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ] ;
    ogit.core:scope "SGO" ;
    ogit.core:tech-contact "arago GmbH" ;
    sh:property
        [
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ogit.foundation:status
        ],
        [
            sh:class ogit.course:Course ;
            sh:description "" ;
            sh:minCount 0 ;
            sh:path ogit.foundation:relates
        ],
        [
            sh:class ogit.course:Series ;
            sh:description "" ;
            sh:minCount 0 ;
            sh:path ogit.foundation:relates
        ] .