#
# Ontology
#
ogit:ontology
    a owl:Ontology;
    rdfs:label "OGIT Ontology";
    dcterms:description """This ontology contains terms that are used to describe OGIT entities, attributes and verbs."""@en;
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.meta";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports purl:ogit
.

#
# Entities
#
ogit.ontology:OntologyAttribute
    a ogit.core:Entity;
    rdfs:label "OntologyAttribute";
    dcterms:description "represents any Attribute definition from OGIT";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:hide "true";
    ogit.core:mandatory-attributes
        ogit.ontology:ontologyId,
        ogit.ontology:ontologyDescription,
        ogit.ontology:ontologyName,
        ogit.ontology:ontologyValidFrom,
        ogit.ontology:ontologyCreator,
        ogit.ontology:ontologyCreated,
        ogit.ontology:ontologyModified,
        ogit.ontology:ontologyAdminContact,
        ogit.ontology:ontologyTechContact;
    ogit.core:optional-attributes
        ogit.ontology:ontologyDeleter,
        ogit.ontology:ontologyHide,
        ogit.ontology:ontologyValidationType,
        ogit.ontology:ontologyValidationParameter;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

