#
# Ontology
#
ogit:person
    a owl:Ontology;
    rdfs:label "OGIT Person";
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.person";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports purl:ogit
.

#
# Entities
#
ogit.person:Account
    a ogit.core:Entity;
    rdfs:label "Account";
    dcterms:description "A account can be e.g. an facebook-account.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:optional-attributes
        ogit.foundation:id,
        ogit.foundation:name,
        ogit.foundation:description,
        ogit.foundation:email;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-09-15";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.


ogit.person:Person
    a ogit.core:Entity;
    rdfs:label "Person";
    dcterms:description "A Person represents a human identity.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-07-27";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:optional-attributes
        ogit.person:firstName,
        ogit.person:lastName,
        ogit.person:middleName,
        ogit.foundation:title,
        ogit.person:alternativeName,
        ogit.foundation:email,
        ogit.person:officePhone,
        ogit.person:otherPhone,
        ogit.person:mobilePhone,
        ogit.person:fax,
        ogit.person:notification,
        ogit.foundation:webPage,
        ogit.foundation:sourceId,
        ogit.foundation:status,
        ogit.person:position;
    ogit.core:indexed-attributes
        ogit.person:firstName,
        ogit.person:lastName,
        ogit.foundation:email,
        ogit.person:alternativeName;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ],
        [
            dcterms:identifier "2";
            dcterms:date "2015-06-11";
            dcterms:description "added optional attribute /ogit/sourceId";
            dcterms:creator "Peter Larem"
        ],
        [
            dcterms:identifier "3";
            dcterms:date "2015-07-27";
            dcterms:description "added optional attribute ogit/middleName, ogit/officePhone, ogit/otherPhone, ogit/mobilePhone, ogit/fax, ogit/webPage, ogit/notification, ogit/status";
            dcterms:creator "Philipp Pelchmann"
        ];
.

ogit.person:Vcard
    a ogit.core:Entity;
    rdfs:label "Vcard";
    dcterms:description """vCard is a specification developed by the IETF for the description of people and organisations. Recently, vCard has been
significantly updated to Version 4 as documented in [RFC6350]. Typically, vCard objects are encoded in its own defined
text-based syntax or XML renderings.""";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:optional-attributes
        ogit.foundation:phone,
        ogit.person:mobilePhone,
        ogit.foundation:streetAddress,
        ogit.person:addressLocality,
        ogit.foundation:postalCode;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

