#
# Verbs
#
ogit._businessProcess:consumes
    a ogit.core:Verb;
    rdfs:label "consumes";
    dcterms:description "This relationship indicates that one consumes result of work from another.";
    dcterms:valid "start=2016-08-08;";
    dcterms:creator "Alexander Ryabtsev";
    dcterms:created "2016-08-08";
    dcterms:modified "2016-08-08";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2016-08-08";
            dcterms:description "initial";
            dcterms:creator "Alexander Ryabtsev"
        ],
        [
            dcterms:identifier "2";
            dcterms:date "2017-02-06";
            dcterms:description "Add edge from person to milestone.";
            dcterms:creator "Alexander Ryabtsev"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.person:Person;
            ogit.core:to ogit.businessProcess:Project
        ],
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.businessProcess:Project
        ],
        [
            ogit.core:from ogit.person:Person;
            ogit.core:to ogit.businessProcess:Milestone
        ],
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.businessProcess:SurveyIteration
        ];
.

