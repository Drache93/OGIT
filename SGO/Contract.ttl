@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:         <http://www.w3.org/2002/07/owl#> .
@prefix dcterms:        <http://purl.org/dc/terms/> .
@prefix vann:           <http://purl.org/vocab/vann/> .
@prefix purl:           <http://www.purl.org/> .
@prefix ogit:           <http://www.purl.org/ogit/> .
@prefix ogit.core:           <http://www.purl.org/ogit/> .
@prefix ogit.contract:  <http://www.purl.org/ogit/> .
@prefix ogit.foundation:     <http://www.purl.org/ogit/> .

#
# Ontology
#
ogit:contract
    a owl:Ontology;
    rdfs:label "OGIT Contract";
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.contract";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports purl:ogit
.

#
# Entities
#
ogit.contract:Contract
    a ogit.core:Entity;
    rdfs:label "Contract";
    dcterms:description "General definition of a contract. Usually used for IT service related contracts but at this level of detail it is not restricted to those.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:mandatory-attributes
        ogit.foundation:name;
    ogit.core:optional-attributes
        ogit.foundation:description,
        ogit.foundation:type,
        ogit.foundation:validFrom,
        ogit.foundation:expirationDate,
        ogit.foundation:status,
                ogit.foundation:deadline,
                ogit.foundation:validTo;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ],
                [
            dcterms:identifier "2";
            dcterms:date "2016-12-19";
            dcterms:description "add attribute ogit/deadline";
            dcterms:creator "stravlos@arago.co"
        ],
                [
            dcterms:identifier "3";
            dcterms:date "2017-01-06";
            dcterms:description "add attribute ogit/validTo";
            dcterms:creator "stravlos@arago.co"
        ];
.

ogit.contract:Penalty
    a ogit.core:Entity;
    rdfs:label "Penalty";
    dcterms:description "Defines the penalty that has to be paid in certain circumstances";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:optional-attributes
        ogit.foundation:description;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.
