@prefix dcterms:       <http://purl.org/dc/terms/> .
@prefix ogit:          <http://www.purl.org/ogit/> .
@prefix ogit._network: <http://www.purl.org/ogit/> .
@prefix ogit.core:     <http://www.purl.org/ogit/> .
@prefix ogit.network:  <http://www.purl.org/ogit/Network/> .
@prefix owl:           <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:          <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann:          <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:network 
	a owl:Ontology; 
	vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit._network";.

#
# Verbs
#
ogit._network:extends
    a ogit.core:Verb;
    rdfs:label "extends";
    dcterms:description "Indicates if one entity extends another one";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.network:NetworkCard;
            ogit.core:to ogit.network:IDSIPS
        ],
        [
            ogit.core:from ogit.network:NetworkCard;
            ogit.core:to ogit.network:Loadbalancer
        ],
        [
            ogit.core:from ogit.network:NetworkCard;
            ogit.core:to ogit.network:Router
        ],
        [
            ogit.core:from ogit.network:NetworkCard;
            ogit.core:to ogit.network:Switch
        ],
        [
            ogit.core:from ogit.network:NetworkCard;
            ogit.core:to ogit.network:WifiAccessPoint
        ],
        [
            ogit.core:from ogit.network:NetworkCard;
            ogit.core:to ogit.network:Slot
        ],
        [
            ogit.core:from ogit.network:SimpleDevice;
            ogit.core:to ogit.network:IDSIPS
        ],
        [
            ogit.core:from ogit.network:SimpleDevice;
            ogit.core:to ogit.network:Loadbalancer
        ],
        [
            ogit.core:from ogit.network:SimpleDevice;
            ogit.core:to ogit.network:Router
        ],
        [
            ogit.core:from ogit.network:SimpleDevice;
            ogit.core:to ogit.network:Switch
        ],
        [
            ogit.core:from ogit.network:SimpleDevice;
            ogit.core:to ogit.network:WifiAccessPoint
        ];
.
