            ogit.core:from ogit.Network:NetworkCard;
            ogit.core:to ogit.Network:IDSIPS
        ],
        [
            ogit.core:from ogit.Network:NetworkCard;
            ogit.core:to ogit.Network:Loadbalancer
        ],
        [
            ogit.core:from ogit.Network:NetworkCard;
            ogit.core:to ogit.Network:Router
        ],
        [
            ogit.core:from ogit.Network:NetworkCard;
