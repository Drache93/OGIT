        ],
        [
            ogit.core:from ogit.Network:SimpleDevice;
            ogit.core:to ogit.Network:Loadbalancer
        ],
        [
            ogit.core:from ogit.Network:SimpleDevice;
            ogit.core:to ogit.Network:Router
        ],
        [
            ogit.core:from ogit.Network:SimpleDevice;
            ogit.core:to ogit.Network:Switch
        ],
        [
            ogit.core:from ogit.Network:SimpleDevice;
            ogit.core:to ogit.Network:WifiAccessPoint
