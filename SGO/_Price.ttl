@prefix dcterms:                <http://purl.org/dc/terms/> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix ogit._price:            <http://www.purl.org/ogit/> .
@prefix ogit.core:              <http://www.purl.org/ogit/> .
@prefix ogit.price:             <http://www.purl.org/ogit/Price/> .
@prefix ogit.serviceManagement: <http://www.purl.org/ogit/ServiceManagement/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann:                   <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:price
    a owl:Ontology;
    vann:preferredNamespacePrefix "ogit._price";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Verbs
#
ogit._price:bills
    a ogit.core:Verb;
    rdfs:label "bills";
    dcterms:created "2015-05-21";
    dcterms:creator "Peter Larem";
    dcterms:description "Indicates if one entity bills another one, e.g. an Invoice bills an Order";
    dcterms:modified "2015-05-21";
    dcterms:valid "start=2015-05-21;";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:allowed [
            ogit.core:from ogit.price:Invoice ;
            ogit.core:to ogit.serviceManagement:Order
        ];
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ];
    ogit.core:tech-contact "arago GmbH" .