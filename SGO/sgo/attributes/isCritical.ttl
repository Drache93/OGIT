    dcterms:valid "start=2015-07-27;";
    dcterms:creator "Aymen Ayoub";
    dcterms:created "2015-07-27";
    dcterms:modified "2015-07-27";
    ogit:admin-contact "arago GmbH";
    ogit:tech-contact "arago GmbH";
    ogit:history
