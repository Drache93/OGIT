@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix ogit.config:            <http://www.purl.org/ogit/> .
@prefix ogit.person:            <http://www.purl.org/ogit/> .
@prefix ogit.org:               <http://www.purl.org/ogit/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .
@prefix ogit.ServiceManagement: <http://www.purl.org/ogit/ServiceManagement/> .

ogit:reports
	a ogit:Verb;
	rdfs:label "reports";
	dcterms:description "Verb showing which entity reports to which another entity.";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2015-05-21;";
	dcterms:creator "Peter Larem";
	dcterms:created "2015-05-21";
	dcterms:modified "2015-05-21";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:history
		[
			dcterms:identifier "1";
			dcterms:date "2015-05-21";
			dcterms:description "initial";
			dcterms:creator "Peter Larem"
		],
		[
			dcterms:identifier "2";
			dcterms:date "2015-08-19";
			dcterms:description "add connection ogit/Person to ogit/Person";
			dcterms:creator "Peter Larem"
		];
	ogit:allowed
		[
			ogit:from ogit.config:Parameter;
			ogit:to ogit.config:Parameter
		],
		[
			ogit:from ogit.person:Person;
			ogit:to ogit.ServiceManagement:Incident
		],
		[
			ogit:from ogit.person:Person;
			ogit:to ogit.ServiceManagement:Problem
		],
		[
			ogit:from ogit.org:Role;
			ogit:to ogit.org:Role
		],
		[
			ogit:from ogit.person:Person;
			ogit:to ogit.person:Person
		];
.
