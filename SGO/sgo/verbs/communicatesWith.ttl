@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix ogit.core:                   <http://www.purl.org/ogit/> .
@prefix ogit.organization:               <http://www.purl.org/ogit/> .
@prefix ogit.person:            <http://www.purl.org/ogit/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit:communicatesWith
    a ogit.core:Verb;
    rdfs:label "communicatesWith";
    dcterms:description "indicates which entities can communicate with other entities";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.organization:Organization
        ],
        [
            ogit.core:from ogit.person:Person;
            ogit.core:to ogit.person:Person
        ];
.
