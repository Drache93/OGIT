@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ogit.Software:          <http://www.purl.org/ogit/Software/> .
@prefix dcterms:                <http://purl.org/dc/terms/> .
@prefix ogit.Network:           <http://www.purl.org/ogit/Network/> .

ogit:transfers
	a ogit:Verb;
	rdfs:label "transfers";
	dcterms:description "Indicates if one entity transfers information to another.";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2015-05-21;";
	dcterms:creator "Peter Larem";
	dcterms:created "2015-05-21";
	dcterms:modified "2015-05-21";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:history
		[
			dcterms:identifier "1";
			dcterms:date "2015-05-21";
			dcterms:description "initial";
			dcterms:creator "Peter Larem"
		];
	ogit:allowed
		[
			ogit:from ogit.Software:SoftwareConnection;
			ogit:to ogit.Network:NetworkLink
		],
		[
			ogit:from ogit.Software:SoftwareConnection;
			ogit:to ogit.Software:Data
		];
.
