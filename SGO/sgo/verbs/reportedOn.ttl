@prefix ogit.Automation:        <http://www.purl.org/ogit/Automation/> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix ogit.core:                   <http://www.purl.org/ogit/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .
@prefix ogit.ServiceManagement: <http://www.purl.org/ogit/ServiceManagement/> .

ogit:reportedOn
    a ogit.core:Verb;
    rdfs:label "reportedOn";
    dcterms:description """Indicates where a failure situation was initially observed. In case an incident record is created from an event management system this relationship points to the entitiy that represents the monitored device/endpoint.
We call it \"reportedOn\" instead of \"observedOn\" to account for the close relation to \"reportedAt\", \"reportedBy\"""";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.ServiceManagement:Order;
            ogit.core:to ogit.Automation:MARSNode
        ],
        [
            ogit.core:from ogit.ServiceManagement:ChangeRequest;
            ogit.core:to ogit.Automation:MARSNode
        ],
        [
            ogit.core:from ogit.ServiceManagement:Incident;
            ogit.core:to ogit.Automation:MARSNode
        ],
        [
            ogit.core:from ogit.ServiceManagement:Problem;
            ogit.core:to ogit.Automation:MARSNode
        ],
        [
            ogit.core:from ogit.ServiceManagement:Ticket;
            ogit.core:to ogit.Automation:MARSNode
        ];
.
