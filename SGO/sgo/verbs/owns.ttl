ogit:owns
    a ogit.core:Verb;
    rdfs:label "owns";
    dcterms:description "This relationship indicates that one entity owns some other.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ],
        [
            dcterms:identifier "2";
            dcterms:date "2015-05-20";
            dcterms:description """Added Connection for `ogit/Forum/Profile` -> `ogit/Forum/KnowledgeBundle`
Added Connection for `ogit/Forum/Profile` -> `ogit/Forum/FeedEntry`
Added Connection for `ogit/Forum/Profile` -> `ogit/Notification`
Added Connection for `ogit/Forum/Profile` -> `ogit/Forum/Topic`
""";
            dcterms:creator ""
        ],
        [
            dcterms:identifier "3";
            dcterms:date "2015-07-27";
            dcterms:description """Added Connection for `ogit/Organization` -> `ogit/Automation/MARSNode`""";
            dcterms:creator "Aymen Ayoub"
        ],
        [
            dcterms:identifier "4";
            dcterms:date "2016-05-17";
            dcterms:description """Added Connection for `ogit/Forum/Group` -> `ogit/Forum/KnowledgeBundle`
Added Connection for `ogit/Forum/Group` -> `ogit/Forum/KnowledgeItemHistory`
Added Connection for `ogit/Forum/Group` -> `ogit/Forum/Topic`
""";
            dcterms:creator "Ben Neal"
        ],
        [
            dcterms:identifier "5";
            dcterms:date "2016-08-03";
            dcterms:description "Added person -> event";
            dcterms:creator "Philipp Pelchmann"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.asset:Asset
        ],
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.Automation:MARSNode
