            ogit:from ogit.Forum:ShortId;
            ogit:to ogit.Forum:Post
        ],
        [
            ogit:from ogit.Forum:ShortId;
            ogit:to ogit.Automation:KnowledgeItem
        ],
        [
            ogit:from ogit.Forum:ShortId;
            ogit:to ogit.Forum:KnowledgeItemHistory
        ];
.
