@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix dcterms:        <http://purl.org/dc/terms/> .
@prefix vann:           <http://purl.org/vocab/vann/> .
@prefix ogit:           <http://www.purl.org/ogit/> .
@prefix ogit.device:    <http://www.purl.org/ogit/> .
@prefix ogit.Datacenter: <http://www.purl.org/ogit/Datacenter/> .

#
# Ontology
#
<http://www.purl.org/ogit/device>
    a owl:Ontology;
    rdfs:label "OGIT Device Vocabulary";
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.device";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports <http://www.purl.org/ogit>
.

#
# Entities
#
ogit.device:Device
    a ogit:Entity;
    rdfs:label "Device";
    dcterms:description "Any type of device";
    # For ranges, see http://dublincore.org/documents/dcmi-period/
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit:admin-contact "arago GmbH";
    ogit:tech-contact "arago GmbH";
    ogit:scope "SGO";
    ogit:parent "http://www.purl.org/ogit/Node";
    ogit:optional-attributes
        ogit:id,
        ogit:name,
        ogit:purchaseDate,
        ogit.Datacenter:moveProductionDate,
        ogit:endOfWarranty,
        ogit:powerConsumption;
    ogit:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.
