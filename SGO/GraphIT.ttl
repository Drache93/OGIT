#
# Ontology
#
<http://www.purl.org/ogit/graphit>
    a owl:Ontology;
    rdfs:label "OGIT GraphIT Vocabulary";
    dcterms:description """This ontology contains terms, internally used by GraphIT."""@en;
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.graphit";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports <http://www.purl.org/ogit>
.

#
# Entities
#
ogit.graphit:Node
    a ogit:Entity;
    rdfs:label "Node";
    dcterms:description """The root node, the 'Big Bang'. The nodes Global, Factual, Actionable and Situational build with this Node the
top level of the SGO.""";
    # For ranges, see http://dublincore.org/documents/dcmi-period/
    dcterms:valid "start=2013-11-01;";
    dcterms:creator "Peter Larem";
    dcterms:created "2013-09-01";
    dcterms:modified "2013-09-01";
    ogit:admin-contact "arago GmbH";
    ogit:tech-contact "arago GmbH";
    ogit:scope "SGO";
    ogit:hide "true";
    ogit:mandatory-attributes
        ogit.graphit:_id,
        ogit.graphit:_type,
        ogit.graphit:_owner,
        ogit.graphit:_modified-on,
        ogit.graphit:_modified-by,
        ogit.graphit:_graphtype,
        ogit.graphit:_is-deleted,
        ogit.graphit:_deleted-on,
        ogit.graphit:_creator,
        ogit.graphit:_created-on,
        ogit.graphit:_version,
        ogit.graphit:_content,
        ogit.graphit:_tags,
        ogit.graphit:_edge_id;
    ogit:history
        [
            dcterms:identifier "1";
            dcterms:date "2013-09-01";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

