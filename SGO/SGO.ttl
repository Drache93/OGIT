#
# Ontology
#
ogit:sgo
    a owl:Ontology;
    rdfs:label "OGIT Semantic Graph Ontology";
    owl:imports purl:ogit;
    owl:versionInfo "__OGIT_VERSION__";
    dcterms:contributor "Fabian Meyer";
    dcterms:creator "arago GmbH";
    dcterms:description "This ontology contains inter-domain knowledge."@en;
    vann:preferredNamespacePrefix "ogit.sgo";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

