@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix dcterms:        <http://purl.org/dc/terms/> .
@prefix ogit:           <http://www.purl.org/ogit/> .
@prefix ogit.core:           <http://www.purl.org/ogit/> .
@prefix ogit._Cost:           <http://www.purl.org/ogit/> .
@prefix ogit.Cost:         <http://www.purl.org/ogit/Cost/> .

#
# Ontology
#
ogit:Cost a owl:Ontology .

#
# Verbs
#
ogit._Cost:assumes
    a ogit.core:Verb;
    rdfs:label "assumes";
    dcterms:description "verb which allows to connect the planning parameters to a planning template";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.Cost:PlanningParameter;
            ogit.core:to ogit.Cost:PlanningTemplate
        ];
.

ogit._Cost:contributes
    a ogit.core:Verb;
    rdfs:label "contributes";
    dcterms:description """Verb associates which entity contributes to another, for example could be used to express a distribution for the percentage
of a cost element, which contributes to another cost element.""";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.Cost:CostElement;
            ogit.core:to ogit.Cost:CostDistribution
        ];
.

ogit._Cost:covers
    a ogit.core:Verb;
    rdfs:label "covers";
    dcterms:description "Verb showing which entity covers another entity.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.Cost:Budget;
            ogit.core:to ogit.Cost:CostElement
        ];
.
