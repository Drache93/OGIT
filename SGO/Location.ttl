@prefix dcterms:                <http://purl.org/dc/terms/> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix ogit.asset:             <http://www.purl.org/ogit/> .
@prefix ogit.automation:        <http://www.purl.org/ogit/Automation/> .
@prefix ogit.core:              <http://www.purl.org/ogit/> .
@prefix ogit.datacenter:        <http://www.purl.org/ogit/Datacenter/> .
@prefix ogit.foundation:        <http://www.purl.org/ogit/> .
@prefix ogit.location:          <http://www.purl.org/ogit/> .
@prefix ogit.organization:      <http://www.purl.org/ogit/> .
@prefix ogit.person:            <http://www.purl.org/ogit/> .
@prefix ogit.serviceManagement: <http://www.purl.org/ogit/ServiceManagement/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix purl:                   <http://www.purl.org/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann:                   <http://purl.org/vocab/vann/> .
@prefix sh:				  <http://www.w3.org/ns/shacl#> .

#
# Ontology
#
ogit:location
    a owl:Ontology;
    rdfs:label "OGIT Location";
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.location";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports purl:ogit
.

#
# Entities
#
ogit.location:Location
    a ogit.core:Entity;
    rdfs:label "Location";
    dcterms:description """A place where some some service is offered or requested. In many cases this will be related to a an address of an
Organization. Similar to http://purl.org/goodrelations/v1#Location""";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    sh:property
        [
            sh:path ogit.foundation:name;
            sh:minCount 1;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:description;
            sh:minCount 0;
            sh:maxCount 1
        ];
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

ogit.location:Region
    a ogit.core:Entity;
    rdfs:label "Region";
    dcterms:description "A region is always something that has a certain geographic extension (a country, a county,  ) whereas ogit/Location usually refers to specific place usually having an address";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    sh:property
        [
            sh:path ogit.foundation:name;
            sh:minCount 0;
            sh:maxCount 1
        ],
        [
            sh:path ogit.foundation:description;
            sh:minCount 0;
            sh:maxCount 1
        ];
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
.

#
# Verbs
#
ogit.location:locatedIn
    a ogit.core:Verb;
    rdfs:label "locatedIn";
    dcterms:description "indicate that something has its place somewhere";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ],
        [
            dcterms:identifier "2";
            dcterms:date "2015-05-12";
            dcterms:description "updated";
            dcterms:creator "Aymen Ayoub"
        ],
        [
            dcterms:identifier "3";
            dcterms:date "2015-07-06";
            dcterms:description "updated";
            dcterms:creator "Aymen Ayoub"
        ],
        [
            dcterms:identifier "4";
            dcterms:date "2015-07-15";
            dcterms:description "updated";
            dcterms:creator "Aymen Ayoub"
        ],
        [
            dcterms:identifier "5";
            dcterms:date "2015-07-20";
            dcterms:description "updated";
            dcterms:creator "Aymen Ayoub"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.automation:MARSNode;
            ogit.core:to ogit.location:Region
        ],
        [
            ogit.core:from ogit.datacenter:Datacenter;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.location:Location;
            ogit.core:to ogit.location:Region
        ],
        [
            ogit.core:from ogit.person:Vcard;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.serviceManagement:Offering;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.asset:Asset;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.organization:Organization;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.person:Person;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.serviceManagement:Incident;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.serviceManagement:ChangeRequest;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.automation:MARSNode;
            ogit.core:to ogit.location:Location
        ],
        [
            ogit.core:from ogit.serviceManagement:SubTask;
            ogit.core:to ogit.location:Location
        ];
.

