#
# Ontology
#
<http://www.purl.org/ogit/configuration>
    a owl:Ontology;
    rdfs:label "OGIT Configuration Vocabulary";
    dcterms:description """This ontology contains terms regarding configurations."""@en;
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.config";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports <http://www.purl.org/ogit>
.

