@prefix dcterms:     <http://purl.org/dc/terms/> .
@prefix ogit:        <http://www.purl.org/ogit/> .
@prefix ogit._forum: <http://www.purl.org/ogit/> .
@prefix ogit.core:   <http://www.purl.org/ogit/> .
@prefix ogit.forum:  <http://www.purl.org/ogit/Forum/> .
@prefix ogit.media:  <http://www.purl.org/ogit/> .
@prefix owl:         <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:        <http://www.w3.org/2000/01/rdf-schema#> .

#
# Ontology
#
ogit:forum a owl:Ontology .

#
# Verbs
#
ogit._forum:accepts
    a ogit.core:Verb;
    rdfs:label "accepts";
    dcterms:description "Indicates if an entity accepts something else.";
    dcterms:valid "start=2015-09-02;";
    dcterms:creator "bmoore@arago.de";
    dcterms:created "2015-09-02";
    dcterms:modified "2015-09-02";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-09-02";
            dcterms:description "initial";
            dcterms:creator "bmoore@arago.de"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:Invite
        ];
.

ogit._forum:configures
    a ogit.core:Verb;
    rdfs:label "configures";
    dcterms:description "The `from` entity, contains configuration data for the to `entity`";
    dcterms:valid "start=2015-04-27;";
    dcterms:creator "cwalker@arago.de";
    dcterms:created "2015-04-27";
    dcterms:modified "2015-04-27";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-04-27";
            dcterms:description "intitial inception";
            dcterms:creator "cwalker@arago.de"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.forum:Configuration;
            ogit.core:to ogit.forum:Profile
        ];
.

ogit._forum:endorses
    a ogit.core:Verb;
    rdfs:label "endorses";
    dcterms:description """This relationship indicates that one entity endorses some other. This is a recommendation or a
social `like`.""";
    dcterms:valid "start=2015-04-27;";
    dcterms:creator "cwalker@arago.de";
    dcterms:created "2015-04-27";
    dcterms:modified "2015-04-27";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-04-27";
            dcterms:description "intitial inception";
            dcterms:creator "cwalker@arago.de"
        ],
        [
            dcterms:identifier "2";
            dcterms:date "2015-11-13";
            dcterms:description "Edit for KnowledgeItemHistory";
            dcterms:creator "bmoore@arago.de"
        ],
        [
            dcterms:identifier "3";
            dcterms:date "2016-05-17";
            dcterms:description "Added edge for `ogit/Forum/Profile` -> `ogit/Forum/Rating`";
            dcterms:creator "Ben Neal"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:KnowledgeBundle
        ],
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:KnowledgeItemHistory
        ],
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:Post
        ],
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:Reply
        ],
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:Rating
        ];
.

ogit._forum:moderates
    a ogit.core:Verb;
    rdfs:label "moderates";
    dcterms:description """Moderation as a verb, in the context of a forum, is a supervisory role. It implies that the
entity takes a responsibility for the maintenance and upkeep of the connected entity.""";
    dcterms:valid "start=2015-04-23;";
    dcterms:creator "cwalker@arago.de";
    dcterms:created "2015-04-23";
    dcterms:modified "2015-04-23";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:cardinality "many2many";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-04-23";
            dcterms:description "Initial Creation";
            dcterms:creator "cwalker@arago.de"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:Topic
        ],
        [
            ogit.core:from ogit.forum:Profile;
            ogit.core:to ogit.forum:KnowledgeBundle
        ];
.

ogit._forum:responds
