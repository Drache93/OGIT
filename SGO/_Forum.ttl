@prefix dcterms:     <http://purl.org/dc/terms/> .
@prefix ogit:        <http://www.purl.org/ogit/> .
@prefix ogit._forum: <http://www.purl.org/ogit/> .
@prefix ogit.core:   <http://www.purl.org/ogit/> .
@prefix ogit.forum:  <http://www.purl.org/ogit/Forum/> .
@prefix ogit.media:  <http://www.purl.org/ogit/> .
@prefix owl:         <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:        <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann:        <http://purl.org/vocab/vann/> .

#
# Ontology
#
ogit:forum
    a owl:Ontology;
    vann:preferredNamespacePrefix "ogit._forum";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/" .

#
# Verbs
#
ogit._forum:accepts
    a ogit.core:Verb;
    rdfs:label "accepts";
    dcterms:created "2015-09-02";
    dcterms:creator "bmoore@arago.de";
    dcterms:description "Indicates if an entity accepts something else.";
    dcterms:modified "2015-09-02";
    dcterms:valid "start=2015-09-02;";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:allowed [
            ogit.core:from ogit.forum:Profile ;
            ogit.core:to ogit.forum:Invite
        ];
    ogit.core:history [
            dcterms:creator "bmoore@arago.de" ;
            dcterms:date "2015-09-02" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ];
    ogit.core:tech-contact "arago GmbH" .

ogit._forum:configures
    a ogit.core:Verb;
    rdfs:label "configures";
    dcterms:created "2015-04-27";
    dcterms:creator "cwalker@arago.de";
    dcterms:description "The `from` entity, contains configuration data for the to `entity`";
