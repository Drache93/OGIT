#
# Ontology
#
ogit:event
    a owl:Ontology;
    rdfs:label "OGIT Event";
    dcterms:creator "arago GmbH";
    dcterms:contributor "Fabian Meyer";
    vann:preferredNamespaceUri "http://www.purl.org/ogit/";
    vann:preferredNamespacePrefix "ogit.event";
    owl:versionInfo "__OGIT_VERSION__";
    owl:imports purl:ogit
.

#
# Entities
#
ogit.event:Alert
    a ogit.core:Entity;
    rdfs:label "Alert";
    dcterms:description "Alerts are used to notify users if certain events happen.";
    dcterms:valid "start=2016-10-26;";
    dcterms:creator "stravlos@arago.de";
    dcterms:created "2016-10-26";
    dcterms:modified "2016-10-261";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:mandatory-attributes
        ogit.foundation:type,
        ogit.foundation:creator,
    ogit.foundation:category;
    ogit.core:optional-attributes
    ogit.foundation:function,
        ogit.foundation:value;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2016-10-26";
            dcterms:description "initial";
            dcterms:creator "stravlos@arago.de"
        ],
                [
            dcterms:identifier "2";
            dcterms:date "2016-11-02";
            dcterms:description "make attributes optional";
            dcterms:creator "stravlos@arago.de"
        ];
.

ogit.event:Event
    a ogit.core:Entity;
    rdfs:label "Event";
    dcterms:description "This entity defines any type of event records, e.g. ITSM/produced by event management systems, Business Process etc.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:scope "SGO";
    ogit.core:parent "http://www.purl.org/ogit/Node";
    ogit.core:mandatory-attributes
        ogit.foundation:name,
        ogit.foundation:id;
    ogit.core:optional-attributes
        ogit.foundation:status,
        ogit.foundation:description,
        ogit.event:severity,
        ogit.foundation:summary,
        ogit.foundation:impact,
        ogit.foundation:priority,
        ogit.event:certainty,
        ogit.event:firstOccurredAt,
        ogit.event:lastOccurredAt,
        ogit.foundation:openedAt,
        ogit.foundation:lastUpdatedAt,
        ogit.event:lastClearedAt,
        ogit.event:expiresAt,
        ogit.event:occurenceCount,
        ogit.foundation:type,
        ogit.ServiceManagement:reportedSource,
        ogit.foundation:category,
        ogit.foundation:instance,
        ogit.event:resource,
        ogit.event:maintenanceState,
        ogit.event:acknowledgeState,
        ogit.event:isRoot;
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ],
        [
            dcterms:identifier "2";
            dcterms:date "2016-08-03";
            dcterms:description "Added Attributes: ogit/summary, ogit/impact, ogit/priority, ogit/certainty, ogit/firstOccurredAt, ogit/lastOccurredAt, ogit/openedAt, ogit/lastUpdatedAt, ogit/lastClearedAt, ogit/expiresAt, ogit/occurenceCount,  ogit/ServiceManagement/reportedSource, ogit/type, ogit/category, ogit/instance, ogit/resource, ogit/maintenanceState, ogit/acknowledgeState, ogit/isRoot";
            dcterms:creator "Philipp Pelchmann"
        ];
.

