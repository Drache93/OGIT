@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix dcterms:        <http://purl.org/dc/terms/> .
@prefix ogit:           <http://www.purl.org/ogit/> .
@prefix ogit.core:           <http://www.purl.org/ogit/> .
@prefix ogit._Software:           <http://www.purl.org/ogit/> .
@prefix ogit.Network:         <http://www.purl.org/ogit/Network/> .
@prefix ogit.Software:         <http://www.purl.org/ogit/Software/> .

#
# Ontology
#
ogit:Software a owl:Ontology .

#
# Verbs
#
ogit._Software:transfers
    a ogit.core:Verb;
    rdfs:label "transfers";
    dcterms:description "Indicates if one entity transfers information to another.";
    dcterms:valid "start=2015-05-21;";
    dcterms:creator "Peter Larem";
    dcterms:created "2015-05-21";
    dcterms:modified "2015-05-21";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:tech-contact "arago GmbH";
    ogit.core:history
        [
            dcterms:identifier "1";
            dcterms:date "2015-05-21";
            dcterms:description "initial";
            dcterms:creator "Peter Larem"
        ];
    ogit.core:allowed
        [
            ogit.core:from ogit.Software:SoftwareConnection;
            ogit.core:to ogit.Network:NetworkLink
        ],
        [
            ogit.core:from ogit.Software:SoftwareConnection;
            ogit.core:to ogit.Software:Data
        ];
.
