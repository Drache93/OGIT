ogit._serviceManagement:effort
    a ogit.core:Attribute;
    rdfs:label "effort";
    dcterms:created "2015-05-21";
    dcterms:creator "Peter Larem";
    dcterms:description """Defines how much time was spent on the actual work for a change, task, or similar entity.
        The values is expected to be in format HH:MM, e.g. 1:00 for 1 hour effort.""";
    dcterms:modified "2015-05-21";
    dcterms:valid "start=2015-05-21;";
    ogit.core:admin-contact "arago GmbH";
    ogit.core:history [
            dcterms:creator "Peter Larem" ;
            dcterms:date "2015-05-21" ;
            dcterms:description "initial" ;
            dcterms:identifier "1"
        ];
    ogit.core:tech-contact "arago GmbH";
    ogit.core:validation-parameter "^(\\d+:\\d\\d)$";
    ogit.core:validation-type "regex" .

